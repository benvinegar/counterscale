        "@remix-run/css-bundle": "^2.8.1",
        "@remix-run/react": "^2.8.1",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "dayjs": "^1.11.10",
        "isbot": "^3.6.8",
        "lucide-react": "^0.302.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "recharts": "^2.10.3",
        "run": "^1.4.0",
        "tailwind-merge": "^2.2.0",
        "tailwindcss-animate": "^1.0.7",
        "ua-parser-js": "^1.0.37"
